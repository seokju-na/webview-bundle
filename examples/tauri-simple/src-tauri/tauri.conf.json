{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri",
  "version": "0.0.0",
  "identifier": "dev.wvb.tauri-simple",
  "build": {
    "beforeDevCommand": "yarn dev"
  },
  "app": {
    "windows": [
      {
        "title": "out",
        "width": 800,
        "height": 600,
        "url": "bundle://out.wvb"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  }
}

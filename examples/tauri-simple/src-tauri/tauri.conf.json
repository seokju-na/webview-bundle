{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri",
  "version": "0.0.0",
  "identifier": "dev.wvb.tauri-simple",
  "build": {
    "beforeDevCommand": "yarn dev"
  },
  "app": {
    "windows": [
      {
        "label": "out",
        "title": "out",
        "width": 800,
        "height": 600,
        "url": "bundle://out.wvb"
      },
      {
        "label": "big",
        "title": "big",
        "width": 800,
        "height": 600,
        "url": "bundle://big.wvb"
      },
      {
        "label": "nextjs",
        "title": "nextjs",
        "width": 800,
        "height": 600,
        "url": "bundle://nextjs.wvb"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}

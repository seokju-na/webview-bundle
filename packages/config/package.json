      "require": "./dist/remote/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}

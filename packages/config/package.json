{
  "name": "@webview-bundle/config",
  "description": "Configuration for webview bundle",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./remote": {
      "import": "./dist/remote/index.js",
      "require": "./dist/remote/index.cjs"
    },
    "./signature": {
      "import": "./dist/signature/index.js",
      "require": "./dist/signature/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/config"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "yarn build",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.932.0",
    "@webview-bundle/node": "workspace:^",
    "rolldown": "1.0.0-beta.49"
  },
  "devDependencies": {
    "@types/node": "22.18.3",
    "execa": "^9",
    "tsdown": "0.15.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}

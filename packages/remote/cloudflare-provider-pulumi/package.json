{
  "name": "@wvb/remote-cloudflare-provider-pulumi",
  "version": "0.0.0",
  "description": "Webview Bundle remote pulumi configuration for Cloudflare",
  "homepage": "https://github.com/webview-bundle/webview-bundle",
  "bugs": {
    "url": "https://github.com/webview-bundle/webview-bundle/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webview-bundle/webview-bundle",
    "directory": "packages/remote/cloudflare-provider-pulumi"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "yarn build-js && yarn build-worker-script",
    "build-js": "tsdown",
    "build-worker-script": "esbuild ./worker-script.ts --minify --bundle --format=esm --platform=neutral --target=node18 --sourcemap=external --outfile=./dist/worker-script.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@wvb/remote-cloudflare-provider": "workspace:^"
  },
  "devDependencies": {
    "@pulumi/cloudflare": "^6.10.0",
    "@pulumi/pulumi": "^3.201.0",
    "@types/node": "catalog:",
    "esbuild": "0.25.3",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@pulumi/cloudflare": "*",
    "@pulumi/pulumi": "*"
  }
}

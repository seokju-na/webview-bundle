{
  "name": "@webview-bundle/cli",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "webview-bundle": "./dist/cli.js",
    "wvb": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">= 18.14.1 < 19 || >= 19.7.0 < 20 || >= 20.0.0"
  },
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "yarn build",
    "clean": "rm -rf dist/",
    "build": "yarn clean && yarn build-js && yarn build-cli",
    "build-cli": "tsdown -c tsdown.cli.config.ts",
    "build-js": "tsdown -c tsdown.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.2",
    "@logtape/logtape": "1.0.4",
    "@webview-bundle/config": "workspace:^",
    "@webview-bundle/node": "workspace:^",
    "ci-info": "^4.2.0",
    "clipanion": "4.0.0-rc.4",
    "file-type": "^21.0.0",
    "hono": "^4.10.3",
    "kleur": "^4.1.5",
    "picomatch": "^4.0.3",
    "supports-color": "^10.0.0",
    "tinyglobby": "^0.2.14",
    "typanion": "^3.14.0"
  },
  "devDependencies": {
    "@types/node": "22.18.3",
    "@types/picomatch": "^4",
    "tsdown": "0.15.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}

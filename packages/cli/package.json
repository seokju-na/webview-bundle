{
  "name": "@webview-bundle/cli",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "webview-bundle": "./dist/main.js",
    "wvb": "./dist/main.js"
  },
  "exports": {
    "./binding": {
      "import": "./binding.js",
      "require": "./binding.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "binding.d.ts",
    "binding.d.cts",
    "binding.js",
    "binding.cjs",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">= 18.14.1 < 19 || >= 19.7.0 < 20 || >= 20.0.0"
  },
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "napi": {
    "binaryName": "binding",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "i686-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "aarch64-apple-darwin",
      "aarch64-linux-android",
      "x86_64-unknown-freebsd",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "armv7-linux-androideabi"
    ]
  },
  "scripts": {
    "prepack": "yarn build",
    "artifacts": "just xtask artifacts napi packages/node-binding && napi artifacts",
    "build": "yarn build-napi && yarn build-js",
    "build-js": "tsdown",
    "build-napi": "yarn build-napi-esm && yarn build-napi-cjs",
    "build-napi-esm": "napi build --platform --release --js=binding.js --dts=binding.d.ts --esm --no-const-enum",
    "build-napi-cjs": "napi build --platform --release --js=binding.cjs --dts=binding.d.cts --no-const-enum",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.2",
    "@logtape/logtape": "1.0.4",
    "ci-info": "^4.2.0",
    "clipanion": "^4.0.0-rc.4",
    "file-type": "^21.0.0",
    "hono": "^4.9.7",
    "kleur": "^4.1.5",
    "picomatch": "^4.0.3",
    "supports-color": "^10.0.0",
    "tinyglobby": "^0.2.14",
    "typanion": "^3.14.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@napi-rs/cli": "3.1.5",
    "@types/node": "22.18.3",
    "@types/picomatch": "^4",
    "tsdown": "0.15.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}

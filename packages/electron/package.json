{
  "name": "@webview-bundle/electron",
  "description": "Webview Bundle API for Electron",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preload": {
      "import": "./dist/preload/index.js",
      "require": "./dist/preload/index.cjs"
    },
    "./renderer": {
      "import": "./dist/renderer/index.js",
      "require": "./dist/renderer/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/electron"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "yarn build",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "electron": ">= 15"
  },
  "dependencies": {
    "@webview-bundle/node": "workspace:^"
  },
  "devDependencies": {
    "@napi-rs/cli": "3.1.5",
    "@types/node": "22.18.3",
    "electron": "^35.7.5",
    "tsdown": "0.15.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}

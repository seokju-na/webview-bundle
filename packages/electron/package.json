{
  "name": "@webview-bundle/electron",
  "description": "Webview bundle API for Electron",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/electron"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 12.22.0 < 13 || >= 14.17.0 < 15 || >= 15.12.0 < 16 || >= 16.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "electron": ">= 15"
  },
  "dependencies": {
    "@webview-bundle/electron-binding": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "22.18.3",
    "electron": "^35.7.5",
    "tsdown": "0.15.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}

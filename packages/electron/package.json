{
  "name": "@webview-bundle/electron",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./preload": "./src/preload/index.ts",
    "./renderer": "./src/renderer/index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./preload": {
        "import": "./dist/preload/index.js",
        "require": "./dist/preload/index.cjs"
      },
      "./renderer": {
        "import": "./dist/renderer/index.js",
        "require": "./dist/renderer/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/electron"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "yarn build",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "electron": ">= 25"
  },
  "dependencies": {
    "@webview-bundle/node-binding": "workspace:^",
    "es-mime-types": "^0.1.4"
  },
  "devDependencies": {
    "@types/mime-types": "^2",
    "@types/node": "22.7.4",
    "@yarnpkg/fslib": "^3.1.0",
    "electron": "^33.0.0",
    "tsdown": "0.12.9",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  }
}

{
  "name": "@webview-bundle/electron",
  "description": "Webview bundle API for Electron",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./binding": {
      "import": "./binding.js",
      "require": "./binding.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "binding.js",
    "binding.cjs",
    "binding.d.ts",
    "binding.d.cts",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/electron"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "napi": {
    "binaryName": "binding",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "i686-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "x86_64-unknown-freebsd",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc"
    ]
  },
  "engines": {
    "node": ">= 12.22.0 < 13 || >= 14.17.0 < 15 || >= 15.12.0 < 16 || >= 16.0.0"
  },
  "scripts": {
    "artifacts": "just xtask artifacts napi packages/electron && napi artifacts",
    "build": "yarn build-napi && yarn build-js",
    "build-napi": "yarn build-napi-esm && yarn build-napi-cjs",
    "build-napi-esm": "napi build --platform --release --js=binding.js --dts=binding.d.ts --esm --no-const-enum",
    "build-napi-cjs": "napi build --platform --release --js=binding.cjs --dts=binding.d.cts --no-const-enum",
    "build-js": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "electron": ">= 15"
  },
  "devDependencies": {
    "@napi-rs/cli": "3.1.5",
    "@types/node": "22.18.3",
    "electron": "^35.7.5",
    "tsdown": "0.15.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}

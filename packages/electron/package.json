{
  "name": "@wvb/electron",
  "description": "Webview Bundle API for Electron",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./preload": {
      "import": "./dist/preload/index.mjs",
      "require": "./dist/preload/index.cjs"
    },
    "./renderer": {
      "import": "./dist/renderer/index.mjs",
      "require": "./dist/renderer/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/seokju-na/webview-bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/webview-bundle",
    "directory": "packages/electron"
  },
  "bugs": {
    "url": "https://github.com/seokju-na/webview-bundle/issues"
  },
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "yarn build",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "electron": ">= 15"
  },
  "dependencies": {
    "@wvb/node": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "electron": "^35.7.5",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
